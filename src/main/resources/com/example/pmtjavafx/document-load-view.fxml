<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.pmtjavafx.controller.DocumentLoadController"
            prefHeight="100.0" prefWidth="200.0"
            >
    <Label fx:id="errorLabel" visible="false">Ошибка! Выбран файл неверного формата</Label>
    <Button AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="50.0" AnchorPane.rightAnchor="50.0" fx:id="loadButton" onAction="#OnLoadButtonClick">Выбрать файл</Button>
</AnchorPane>
